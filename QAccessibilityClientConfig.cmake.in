@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
find_dependency(Qt5Core @QT_MIN_VERSION@)
find_dependency(Qt5Widgets @QT_MIN_VERSION@)

# Thanks to the generated exported target, simply use qaccessibilityclient
# e.g. target_link_libraries(mytarget qaccessibilityclient)
include("${CMAKE_CURRENT_LIST_DIR}/QAccessibilityClient.cmake")

# Backward compatibility:
set(QACCESSIBILITYCLIENT_VERSION_MAJOR @QACCESSIBILITYCLIENT_VERSION_MAJOR@)
set(QACCESSIBILITYCLIENT_VERSION_MINOR @QACCESSIBILITYCLIENT_VERSION_MINOR@)
set(QACCESSIBILITYCLIENT_VERSION_PATCH @QACCESSIBILITYCLIENT_VERSION_PATCH@)
set(QACCESSIBILITYCLIENT_VERSION @QACCESSIBILITYCLIENT_VERSION_MAJOR@.@QACCESSIBILITYCLIENT_VERSION_MINOR@.@QACCESSIBILITYCLIENT_VERSION_PATCH@)

get_target_property(QACCESSIBILITYCLIENT_INCLUDE_DIR @QACCESSIBILITYCLIENT_LIB_NAME@ INTERFACE_INCLUDE_DIRECTORIES)
set(QACCESSIBILITYCLIENT_LIBRARY             @QACCESSIBILITYCLIENT_LIB_NAME@)

